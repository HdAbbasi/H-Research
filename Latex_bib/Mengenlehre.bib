@Article{Ro92,
  author =       "Judith Roitman",
  title =        "The Uses of Set Theory",
  journal =      "Mathematical Intelligencer",
  year =         1992,
  volume =       14,
  number =       1,
  pages =        "63--69",
  annote =       "Folgende Anwendungen werden vorgestellt:

                  -- ``Das Ideal der kompakten Operatoren eines
                  unendlich-dimensionalen separablen komplexen
                  Hilbert-Raumes ist die Summer zweier echt
                  enthaltenen Ideale.'' ist unabhaengig in ZFC.

                  -- Eine abelsche Gruppe ist frei gdw sie eine
                  diskrete Norm hat. (``Norm'' ist ganz analog dem
                  Begriff fuer Vektorraeume erklaert, wobei die
                  Skalare ganze Zahlen sind. ``Diskret'' heisst, dass
                  Normen von Elemente ausser dem neutralen einen
                  festen Mindest-Abstand von Null haben.)

                  -- Sei ein ``netter Raum'' ein kompakter metrischer
                  Weg- und lokal Weg-zusammenhaengender Raum. Shelah
                  bewies, dass die additive Gruppe der rationalen
                  Zahlen nicht die Fundamental-Gruppe eines ``netten
                  Raumes'' sein kann, als Korollar von folgendem
                  Resultat:
                  Die Fundamental-Gruppe eines netten Raumes ist
                  entweder endlich erzeugt oder hat die Kardinalitaet
                  des Kontinuums.

                  -- etwas zu Hawaiianischen Ohrringen und starker
                  Homologie

                  -- Es gibt einen nicht-separablen Banach-Raum, so
                  dass jeder Operator (meint wohl stetige
                  Endomorphismen) eine Summe aus einem Operator mit
                  separablem Wertebereich und einem skalaren
                  Vielfachen der Identitaet ist.

                  -- Man betrachte die freie links-distributive
                  Algebra mit genau einem erzeugenden Element. Eine
                  (best.) Relation zwischen zwei Elementen wird dadurch
                  erklaert, dass das eine Element Komponente einer
                  Darstellung des anderen Elements ist. Aus einem
                  Axiom ueber grosse Kardinalzahlen folgt nun, dass
                  diese Relation eine lineare Ordnung ist."
}



@InProceedings{Sc74,
  author =        "Dana Scott",
  title =         "Axiomatizing Set Theory",
  booktitle = {Axiomatic Set Theory II},
  editors =   {Thomas J. Jech},
  volume =        "13, Part 2",
  series =        "Proceedings of Symposia in Pure Mathematics",
  pages =         "207--214",
  year =          1974,
  organization = "American Mathematical Society",
  annote =       "Es wird ein ``natuerliches'' Axiomensystem der
                  Mengenlehre vorgestellt, ohne die ``ueblichen ad-hoc
                  Axiome''.

                  Es gibt drei Typen: Beliebige Objekte, Mengen
                  (nicht-Mengen sind die ``Urelemente''), und
                  Stufen (spezielle Mengen, die den Stufen der
                  kumulativen Hirarchie entsprechen). Axiome sind:

                  -- Extensionalitaet

                  -- Komprehension

                  -- Akkumulation (eine Stufe enthaelt ein Objekt
                  genau dann, wenn es ein Urelement ist, oder Element
                  oder Teilmenge einer ``frueheren'' Stufe (einer
                  Stufe, die Element der betrachteten ist))

                  -- Einschraenkung (zu jedem Objekt gibt es eine
                  Stufe, von der das Objekt Teilmenge ist) (``im
                  wesentlichen das Fundiertheitsaxiom'')

                  Die ueblichen Erzeugungs-Prozesse fuer Mengen sind
                  in diesem Kontext auf ``Elemente'' anwendbar
                  (Objekte, die Elemente von Mengen sind, haben
                  z.B. Potenzmengen).

                  Das Unendlichkeitsaxiom zusammen mit dem Schema der
                  Ersetzung wird durch das Schema der Reflexion
                  ersetzt (zu jeder Eigenschaft ein Axiom, dass die
                  Existenz einer Stufe garantiert, so dass alle
                  Elemente dieser Stufe mit jener Eigenschaft auch die
                  auf diese Stufe relativierte Eigenschaft haben)."
}


