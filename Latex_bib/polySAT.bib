@Article{PT82,
  author =       "Svatopluk Poljak and Daniel Turz{\'{\i}}k",
  title =        "A polynomial algorithm for constructing a large bipartite subgraph with an application to a satisfiability problem",
  journal =      "Canadian Journal of Mathematics",
  year =         1982,
  volume =       34,
  number =       3,
  pages =        "519--524",
  annote =       "Zunaechst wird ein Polynomialzeit-Algorithmus
                  angegeben, der eine bestimmte untere Schranke fuer
                  die maximale Anzahl von Kanten in einem bipartiten
                  Teilgraphen eines ungerichteten zusammenhaengenden
                  Graphen ohne Schlingen auch realisiert.
                  Angewendet wird dieses Ergebnis auf das Problem der
                  Bestimmung von $s(F)$, der maximalen Anzahl
                  (simultan) erfuellbarer Klauseln einer
                  2-Klauselmenge $F$. Allgemein erhalten sie
                     $ s(F) \geq 3/4 c_2(F) + 1/2 c_1(F) $,
                  wobei $c_i$ die Anzahl der $i$-Klauseln ist.
                  {Dieses Resultat erhaelt man auch auf einfache Art
                  durch den Greedy-Algorithmus.}
                  Gibt es in $F$ nicht zwei 2-Klauseln mit den
                  gleichen Variablen, so koennen sie schaerfer
                     $ s(F) \geq 3/4 c_2(F) + 1/2 c_1(F) +
                       1/4 \ceil 1/2 (n - k) \rceil $
                  beweisen, wobei $n$ die Anzahl der Variablen ist,
                  und $k$ die Anzahl der Zusammenhangskomponenten des
                  Graphen mit den Variablen als Ecken und einer Kante
                  zwischen 2 Knoten gdw. es eine 2-Klausel gibt, die
                  genau diese beiden Variablen enthaelt (1-Klauseln
                  werden bei dieser Konstruktion anscheinend
                  ignoriert).
                  Zum Vergleich zitieren sie das Resultat (wohl
                  [Garey/Johnson]), wonach die Entscheidung
                     $ s(F) \geq 7/10 c(F) $
                  wobei $c = c_1 + c_2$ die Anzahl der Klauseln
                  insgesamt ist, NP-vollstaendig ist."
}

