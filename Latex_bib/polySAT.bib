@Article{SAFS95,
  author =       "John S.~Schlipf and Fred S.~Annexstein and John V.~Franco and R.P.~Swaminathan",
  title =        "On finding solutions for extended {H}orn formulas",
  journal =      "Information Processing Letters",
  year =         1995,
  volume =       54,
  number =       3,
  pages =        "133--137",
  month =        {May},
  doi =          {10.1016/0020-0190(95)00019-9},
  annote =       "Pdf vorhanden.
                  In \cite{CH91} wurden ``extended Horn formulas''
                  eingefuehrt. In diesem Artikel wird der Algorithmus
                  SLUR (``single lookahead unit resolution'')
                  eingefuehrt, ein unvollstaendiger quadratischer
                  SAT-Algorithmus
                  (der aber korrekt ist, d.h. wenn ``SAT'' oder
                  ``UNSAT'' ausgegeben wird, so ist dies korrekt ---
                  andernfalls ist die Ausgabe ``give up''),
                  der Eingaben aus der Klasse der ``hidden extended
                  Horn'' wie ``balanced'' (\cite{CC92}) entscheidet.
                  Der Algorithmus basiert auf drei Eigenschaften der
                  betrachteten Klassen:
                  1. Jede Klauselmenge, die weder die leere Klausel
                  noch eine 1-Klausel enthaelt, ist erfuellbar.
                  2. Die Klasse ist abgeschlossen gegen Anwendung
                  partieller Belegungen.
                  3. Unerfuellbarkeit kann mit 1-Klauseln-Resolution
                  bewiesen werden. (Dies folgt mit 1 und 2 (!))
                  (Eigenschaft 2 ist mir nicht klar fuer die
                  ``balancierten'' Formeln, aber der Algorithmus
                  funktioniert in jedem Falle, denn die wesentliche
                  zugrundeliegende Klasse, wo das zu der IP-Aufgabe
                  gehoerende Polytop ganzzahlige Eckpunkte hat, ist
                  abgeschlossen gegen Einsetzungen (!).)
                  SLUR:
                  Phase 1: Einer-Klauseln-Reduktion wird
                  durchgefuehrt. Falls die leere Klausel auftritt, so
                  Ausgabe ``UNSAT''. (Da der Algorithmus im weiteren
                  ``raet'' (ohne Ueberpfuefung), kann im weiteren
                  keine Unerfuellbarkeit mehr festgestellt werden.)
                  Phase 2: Waehle eine Variable $v$. Setze $F_{0/1}$ als
                  das Resultat von Belegung von $v$ und
                  anschliessender 1-Klauseln-Reduktion. Falls sowohl
                  $F_0$ als auch $F_1$ die leere Klausel enthalten, so
                  gebe auf. Andernfalls waehle $F_{\ve}$ ohne leere
                  Klausel, und mache damit weiter.
                  Ist schliesslich keine Variable mehr vorhanden, so
                  ist die Ausgabe (natuerlich) ``SAT''.
                  Eine Formel wird von SLUR ``geloest'', falls fuer
                  alle Wahlen der Algorithmus niemals aufgibt.
                  Diese Klasse, die an unerfuellbaren Klauselmengen
                  genau die enthaelt, die durch 1-Klauseln-Resolution
                  widerlegbar sind, enthaelt alle umbenannten
                  ``extended Horn'' und alle ``balanced''
                  Klauselmengen.
                  (Fuer ``extended Horn'' folgt dies unmittelbar mit
                  den obigen zwei Eigenschaften.)
                  Und, natuerlich, loest der Algorithmus auch
                  echt mehr als diese beiden Klassen."
}

@Article{Kl93,
  author =       "Hans {Kleine B{\"u}ning}",
  title =        "On generalized {H}orn formulas and {$k$}-resolution",
  journal =      "Theoretical Computer Science",
  year =         1993,
  volume =       116,
  number =       2,
  pages =        "405--413",
  month =        {August},
  doi =          {10.1016/0304-3975(93)90331-M},
  annote =       "Pdf-Datei vorhanden.
                  Erweitert die Klassen $\Gamma_k$ aus \cite{GS88},
                  die ja nur aus erfuellbaren Klauselmengen bestanden,
                  einfach durch Weglassen der
                  Erfuellbarkeits-Einschraenkung zu $H_{k+1}$ (mit der
                  (iterierten) Bedingung ueber die Existenz von
                  Variablenmengen-Ketten).
                  Fuehrt $k$-Resolution ein: Mindestens eine
                  Elternklausel ist immer eine Klausel der Laenge
                  hoechstens $k$.
                  (Dies halte ich fuer zu stark, d.h., nicht mehr
                  polynomial behandelbar.)
                  Theorem 3.3: $k$-Resolution ist vollstaendig fuer
                  $H_k$.
                  (Dies folgt einfach daraus, dass $k$-Resolution
                  (meine) $k$-Input-Resolution simuliert, und dass
                  wiederum $k$-Input-Resolution vollstaendig ist fuer
                  $H_k$, was einfach daraus folgt, dass dies fuer $k =
                  1$ (der Horn-Fall) gilt, und fuer eine Formel aus
                  $H_k$ eine Variable existiert, so dass Belegung mit
                  richtig gewaehltem Wahrheitswert in einer Formel aus
                  $H_{k-1}$ resultiert (waehrend die andere Belegung
                  in $H_k$ bleibt).)
                  Korollar 3.4: Eine Formel aus $H_k$ ohne $k$-Klausel
                  ist erfuellbar.
                  Theorem 3.5: Fuer alle $k > 1$ gibt es $F \in H_K
                  \setminus SAT$, so dass $(k-1)$-Resolution nicht die
                  leere Klausel aus $F$ beweisen kann.
                  Lemma 3.6: Sei $F \in H_k \cap SAT$ und $F \models
                  L$ fuer ein Literal $L$. Dann gibt es eine
                  $(k+1)$-Resolution von $L$ aus $F$.
                  Theorem 4.1 (\cite{YD83}) $F \in H_2$ ist
                  unerfuellbar gdw es eine
                  ``LLRI''-Resolutionwiderlegung von $F$ gibt.
                  (LLRI fuer ``linear-layered-resolution'')
                  Theorem 4.2 2-Resolution ist echt staerker als
                  LLRI-Resolution.
                  Er schliesst mit den Worten:
                  ``... the set of formulas for which a 2-resolution
                  refutation can be found is essentially greater than
                  $H_2$. In order to close this gap and to
                  characterize the $k$-resolution, one might look for
                  a natural extension of $H_2$.''
                  Alles ziemlich simpel."
}

