@Article{EKM95,
  author =       "Thomas Eiter and Pekka Kilpel{\"{a}}inen and Heikki Mannila",
  title =        "Recognizing renamable generalized propositional {H}orn formulas is {NP}-complete",
  journal =      "Discrete Applied Mathematics",
  year =         1995,
  volume =       59,
  pages =        "23--31",
  annote =       "Sei $\Gamma_i$ die Hierarchie von Klauselmengen nach
                  Gallo/Scutella, wobei $\Gamma_0$ die Klasse der
                  Horn-Formeln ist.

                  Sei $R\Gamma_i$ die Klasse der umbenannten
                  $\Gamma_i$ (Umklappungen reichen natuerlich aus).

                  $R\Gamma_0$ ist polynomial entscheidbar, aber
                  $R\Gamma_i$ fuer $i \geq 1$ ist NP-vollstaendig
                  (bzgl. Erkennung).
                  Der Beweis reduziert $R\Gamma_1$ auf das ``Exact
                  Hitting Problem'':
                  Gegeben $\{1, \ldots, n\}$ und $F_1, \ldots, F_m
                  \subseteq \{1, \ldots, n\}$, wobei noch $|F_i| \in
                  \{2,3\}$. Gesucht ist $H \subseteq \{1, \ldots,
                  n\}$, so dass $|H \cap F_i| = 1$ fuer alle $i$ gilt.
                  Der Schlusssatz lautet:
                  ``An interesting open problem is characterizing the
                  classes of formulas whose renamed versions can be
                  recognized in polynomial time.''"
}

@Article{GS88,
  author =       "Giorgio Gallo and Maria Grazia Scutell{\`{a}}",
  title =        "Polynomially solvable satisfiability problems",
  journal =      "Information Processing Letters",
  year =         1988,
  volume =       29,
  pages =        "221--227",
  month =        "November",
  annote =       "Die Klassen
                     $\Gamma_k \subseteq SAT$
                  fuer $k \geq 0$ werden eingefuehrt, wobei $\Gamma_0$
                  die Klasse der erfuellbaren Horn-Klauselmengen ist,
                  $\Gamma_1$ die Klasse der erfuellbaren
                  verallgemeinerten Horn-Formeln aus \cite{YD83}, und
                  allgemein $\Gamma_k$ fuer $k \geq 1$ mit \cite{Kl93}
                  wie folgt gegeben ist:
                  Eine erfuellbare Klauselmenge $F$ ist in $\Gamma_k$
                  gdw es eine Anordnung $F = \{C_1, \ldots, C_m\}$ der
                  Klauseln sowie Variablenmengen $V_1, \ldots, V_m$
                  gibt, so dass $V_i \subseteq V_{i+1}$ fuer $i < m$ gilt
                  ($V_i = \emptyset$ ist natuerlich zugelassen) und
                  $\{C_1 \setminus V_1, \ldots, C_m \setminus V_m\}
                  \in \Gamma_{k-1}$.
                  Es wird bewiesen, dass sowohl Mitgliedschaft in
                  $\Gamma_k$ als auch Berechnug einer erfuellenden
                  Belegung fuer $\Gamma_k$ in $O(n^* \cdot n^k)$
                  Schritten geleistet werden kann, wobei $n^*$ die
                  Anzahl der Literalvorkommen in der Eingabe ist.
                  (Aus den Beweisen wird deutlich, dass es sich um
                  schwache (missglueckte) Formen meiner Formelnklasse
                  handelt. Ueberhaupt ist die Beschraenkung auf
                  erfuellbare Klauselmengen ganz unnoetig.)"
}

