@Article{St89,
  author =       "Gunnar St{\r{a}}marck",
  title =        "A note on the computational complexity of the pure classical implicational calculus",
  journal =      "Information Processing Letters",
  year =         1989,
  volume =       31,
  pages =        "277-278",
  annote =       "Hauptergebniss:

                  Jede Formel ueber $\neg, \und, \or, \la$ kann in
                  Polynomial-Zeit in eine TAUT-equivalente Formel
                  ueber $\la$ transformiert werden.

                  Hilfsmittel:

                  Lemma: Jede Formel ueber $\neg, \und, \or, \la$ kann
                  in Poly-Zeit in eine aequivalente Formel ueberfuehrt
                  werden, die, wenn sie ueberhaupt ein
                  Negationszeichen enthaelt, dann als aeusserste
                  Verknuepfung. (Hierfuer ist $\la$ noetig.)

                  In der gewoehnlichen Weise wird eine Formel in eine
                  Konjunktion von Aequivalenzen ueberfuehrt (mit neuen
                  Variablen).

                  $(P \oder Q) \lra ((P \ra Q) \ra Q)$.

                  Der Umgang mit $\und$ und $\la$ ist offensichtlich,
                  und schliesslich
                  $((a \und b) \ra v) \lra (a \la (\b \ra v))$."
}

@Article{Vl93,
  author =       "Frank Vlach",
  title =        "Simplification in a Satisfiability Checker for {VLSI} Applications",
  journal =      "Journal of Automated Reasoning",
  year =         1993,
  volume =       10,
  number =       1,
  pages =        "115-136",
  month =        "February",
  annote =       "Eine Fortfuehrung von \cite{Ge88}. Es werden mehr
                  Regeln zur Vereinfachung gegeben, fuer beliebige
                  aussagenlogische Formeln ueber einer erweiterten
                  Basis (u.a. um ``at least two'' zur schnellen
                  Bearbeitung von PHP).

                  Leider findet sich keine genaue Formulierung von
                  ``simplification'' (entsprechend einem positiven
                  Abstand), aber gemeint koennte das folgende sein:

                  Notwendig ist, dann sich fuer keine Variable die
                  Anzahl ihrer Vorkommen erhoeht. Hinreichend ist zum
                  einen, dass sich fuer eine Variable diese Anzahl in
                  der Tat vermindert, oder dass sich fuer eine
                  Variable die Anzahl ``polarisierter'' Vorkommen
                  erhoeht.

                  Polarisiert heisst hier, dass das Vorkommen entweder
                  positiv oder negativ ist, ansonsten heisst das
                  Vorkommen ``neutral'' (z.B. sind $a,b$ in $ a \lra b$
                  neutral).

                  Moeglichst viele Verknuepfungen wurden inkorporiert,
                  um moeglichst viele Vereinfachungen aufgrund
                  struktureller Eigenschaften einzufangen.

                  Gegenueber \cite{Ge88} ist jedoch der
                  Verzweigungsteil nicht diskutiert (es wird auf einen
                  anderen Artikel ueber ``gerichtetes Backtracking''
                  verwiesen).

                  Die Reduktionsregeln gruppieren sich wie folgt:

                  1. Wahrheitswert-Ausrechnungen

                  2. Vereinfachungen, die sich auf Variablen mit genau
                  einem Vorkommen beziehen (bringt nur etwas fuer die
                  Aequivalenz)

                  3. Vereinfachungen fuer Variablen mit nur wenigen
                  Vorkommen (neben wiederum Regeln fuer die
                  Aequivalenz nur die ``(1,1)-DP'')

                  4. Vereinfachungen, die sich auf Polaritaet beziehen
                  (z.B. die entsprechenden Verallgemeinerungen purer
                  Literale).

                  Fuer CNF ergeben sich wohl nur die altbekannten
                  Reduktionen.

                  Auf dass der im Artikel vorgestellte Algorithmus
                  ``INSTEP'' die Schubfachformeln polynomial
                  entscheiden kann, wird ein ``mindestens
                  zwei''-Operator in die Sprache integriert, und die
                  Zurueckfuehrung vom Niveau $n$ auf Niveau $n-1$
                  geschieht mittels einer Variante von Bibels
                  ``WLG''-Regel (``without loss of generality''):

                  Theorem 12: Seien $v,w$ Variablen, $\psi$ eine
                  Formel, und $\Gamma$ eine Formelmenge (konjunktiv
                  gelesen). Existiert dann eine Umbenennung $f$ der
                  Variablen, so dass $f(v) = w$, und fuer jedes $\phi
                  \in \Gamma$ das Bild $f(\phi)$ nach Umordnung wieder
                  $\phi$ ergibt, so sind die Formeln $(v \oder w \oder
                  \psi) \und \Gamma$ und $(w \oder \psi) \und \Gamma$
                  SAT-aequivalent.

                  Fuer CNF sehr wahrscheinlich nichts Neues, und wohl
                  auch fuer allgemeine Formeln, hier aber immerhin eine
                  konsequente Sammlung."
}

