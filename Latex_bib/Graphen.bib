@InProceedings{FRV77,
  author =       "P. Flajolet and J.C. Raoult and J. Vuillemin",
  title =        "On the average number of registers required for evaluating arithmetic expressions",
  pages =        "196-205",
  booktitle =    "18th Annual Symposium on Foundations of Computer
                  Science",
  year =         1977,
  annote =       "Es wird die Eingabe-Hoehe $h(T)$ fuer binaere Baeume
                  betrachtet. Es wird die obere Schranke
                     $ h(T) \le \log_2 \nlvs(T) $
                  angegeben.  Sei $M_m$ die Menge aller binaeren
                  Baeume mit $m$ inneren Knoten (also mit $m+1$
                  Blaettern). Betrachte die Gleichverteilung auf
                  $M_m$. Fuer die erwartete oder durchschnittliche
                  Eingabe-Hoehe
                    $ A_m = \sum_{T \in M_m} h(T) / |M_m| $
                  wird gezeigt:
                    $ A_m = \log_4 m + 0.292.. +E(\log_4 m) + o(1) $
                  wobei $E$ eine stetige Funktion mit Periode 1 ist,
                  deren Durchschnittswert $0$ ist, und deren Amplitude
                  weniger als $0.05$ ist.
                  Unabhaengig wurde das gleiche Ergebnis in
                  \cite{Ke77} erzielt."
}

