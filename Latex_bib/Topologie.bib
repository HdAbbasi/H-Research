@Article{He46,
  author =       "Edwin Hewitt",
  title =        "On two problems of {U}rysohn",
  journal =      "Annals of Mathematics",
  year =         1946,
  volume =       47,
  number =       3,
  pages =        "503-509",
  month =        "July",
  annote =       "Kopiert.

                  Urysohn (\cite{Ur25}) bewies die Existenz eines abzaehlbaren
                  Hausdorff-Raumes, auf dem jede stetige reellwertige
                  Funktion konstant ist (der also zusammenhaengend
                  sein muss).

                  Es werden nun zwei Verschaerfungen bewiesen:

                  Zu jeder Kardinalzahl, die nicht Summe von
                  abzaehlbar vielen echt kleineren Kardinalzahlen ist,
                  existiert ein regulaerer topologischer Raum, auf dem
                  jede stetige reellwertige Funktion konstant ist.

                  Es gibt einen abzaehlbaren Urysohn-Raum, auf dem
                  jede stetige reellwertige Funktion konstant ist.

                  (Einen abzaehlbaren regulaeren Raum mit dieser
                  Eigenschaft kann es nicht geben, da jeder
                  zusammenhaengende regulaere Raum mit mind. zwei
                  Punkten ueberabzaehlbar ist (\cite{Ur25}).)"
}


@Article{Her65,
  author =       "Horst Herrlich",
  title =        "{Wann sind alle stetigen Abbildungen in $Y$ konstant?}",
  journal =      "Mathematische Zeitschrift",
  year =         1965,
  volume =       90,
  pages =        "152-154",
  annote =       "Kopiert.

                  Ausgehend u.a. von \cite{He46} wird ein relativ
                  einfacher Beweis des folgenden Satzes gegeben:

                  Ein topologischer Raum $Y$ ist genau dann
                  $T_1$-Raum, wenn es einen regulaeren Raum $X$ mit
                  mindestens zwei Punkten gibt, so dass jede stetige
                  Abbildung von $X$ nach $Y$ konstant ist."
}

@Article{Ro97,
  author =       "Peter Roeper",
  title =        "Region-based Topology",
  journal =      "Journal of Philosophical Logic",
  year =         1997,
  volume =       26,
  pages =        "251-309",
  annote =       "Auf der Basis des Begriffes einer ``Bereiches'' wird
                  ein Axiomensystem aufgestellt, dass genau die lokal
                  kompakten Hausdorff-Raeume charakterisiert.
                  Grundbegriffe: Bereiche (bilden eine Boole'sche
                  Algebra), eine zweistellige Relation ``beruehrt''
                  zwischen Bereichen, und das einstellige Praedikat
                  ``beschraenkt'' fuer Bereiche.
                  In der Interpretation entsprechen die Bereiche genau
                  denn ``regulaer-abgeschlossenen'' Mengen (Mengen,
                  die identisch mit dem Abschluss ihres Inneren sind),
                  ``beruehrt'' heisst ``nicht-leerer Durchschnitt'',
                  und ``beschraenkt'' wird ``kompakt''
                  ``Philosophisch relevant'' scheint hier die
                  Punktlosigkeit der Axiomatisierung mittels des
                  Begriffes von ``Bereichen'' zu sein.
                  Na ja, vielleicht kann man hier etwas topologische
                  Intuition finden.
                  "
}

