@Book{Mi71,
  author =       "Leonid Mirsky",
  title =        "Transversal Theory",
  publisher =    "Academic Press",
  year =         1971,
  annote =       "Viel ueber ``unendliche Situationen'' (mittels
                  Tychonoff's Theorem). Der ``defect'' einer
                  partiellen Transversalen X einer Mengenfamilie
                  (bzw. eines bipartiten Graphen) ist einfach die
                  Differenz $|I| - |X|$ der Anzahl der Mengen
                  (insgesamt) und der Groesse der partiellen
                  Transversalen (bzw. ist $I$ die Groesse der ``linken
                  Seite'' des bipartiten Graphen).
                  Fuer das Transversalmatroid S ist dieser ``defect''
                  fuer eine unabhaengige Menge U einfach $|S| - |U|$
                  (was gelegentlich auch der ``Corang'' von $U$
                  genannt wird, welcher fuer eine beliebige Teilmenge
                  A von S die Groesse $|S| - Rang(A)$ ist --- dieser
                  Corang fuer eine Teilmenge A eines
                  Transversalmatroids S, wenn A nicht auf S bezogen
                  wird, sondern selbst ein Matroid bildet, ist dann
                  gerade der ``Defekt'' aus \cite{Li68})."
}

